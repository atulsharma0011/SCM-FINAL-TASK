li=[["maggi",12],
     ["pasta",20],
     ["vinegar",35],
     ["wiper" ,150] ]
bill=""
b=0
for i in range (1):
    print("name of item no.",i+1," :")
    item=input()
    for j in range (len(li)):
        if li[j][0]==item:
            c1=li[j][1]
            b=b+c1
print("Total Amount to be payed","=", bill," = Rs",b)
